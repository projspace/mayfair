	width:437px;
	margin:16px 0 0 0;
	border-top:1px solid #bcbdc0;
}
.contactForm .detail-section label {
	width:206px;
	margin-right:0px;
}
.contactForm h3 {
	padding:35px 0 18px 0;
	float:left;
}
.contactForm h3 a {
	padding:0 0 0 45px;
	font-size:14px;
	line-height:16px;
	color:#959595;
	text-transform:none;
	font-family:Georgia, "Times New Roman", Times, serif;
}
.contactForm h3 input {
	float:right;
}
.contactForm .custom-radio input {
	left: 20px;
	top: 40px;
}
#tab-content {
	border: 1px solid #CDCED0;
	float: left;
	min-height: 400px;
	padding: 30px 29px;
	width: 874px;
}
.tab-wrapper h1 {
	color:#b5985b;
	font-size:24px;
	line-height:24px;
	padding:49px 0 40px 0;
	text-transform:uppercase;
}
.main-nav > li > a.Men, .main-nav > li > a.woman {
	padding-top:12px;
	min-height:19px;
}
.index-bg {
	background:url(../images/index-bg.png) repeat;
}
.index-bg .main {
	background:#fff;
	padding:0 20px;
	width:1259px;
}
.index-bg #header, .inner-header {
	background:none;
}
/*checkout css ends here*/
.index-bg .inner-header {
	width:1299px;
	margin:auto;
	padding-top:18px;
	background:#607C74 !important;
}
.index-bg #header {
	padding-top:0px;
}
.index-bg .main-nav > li > a {
	padding:5px 7px;
}
.index-bg .main-nav > li > a.Men, .index-bg .main-nav > li > a.woman {
	padding-top:12px;
}
#product-page .overflow {
	padding-top:0px;
}
#product-preview .overflow {
	padding-top:0px;
}
#cont-2 {
	font-size:24px;
}
.checkbox {
	height: 16px;
	width: 11px;
	clear:left;
	float:left;
	margin: -2.5px 0 0 0;
	padding: 0 5px 0 0px;
	background: url(../images/check_box.jpg) no-repeat;
	cursor: default;
	text-align:left;
}
.checkbox input {
	display: none;
}
.checkbox input.show {
	display: inline;
}
#ui-datepicker-div {
	width:197px;
	margin:-24px 0 0 0;
	display:none;
	background: #fff;
}
.ui-datepicker-prev {
	border-right: 2px solid #A0A0A0;
	background:url(../images/cal-left.jpg) left top no-repeat;
	float:left;
}
.ui-datepicker-next {
	border-left: 2px solid #A0A0A0;
	background:url(../images/cal-rite.jpg) right top no-repeat;
	float:right;
}
.ui-datepicker-header {
	width:197px !important;
	background: #fff;
}
.ui-corner-all {
	width:23px;
	height:24px;
	margin-bottom:2px;
	margin-right:4px;
}
.ui-icon-circle-triangle-w, .ui-icon-circle-triangle-e {
	display:none;
}
.ui-datepicker-title {
	overflow:hidden;
	width:130px;
	text-align:center;
	color: #262626;
	background:#fff;
	font-size: 14px;
}
.ui-datepicker-title span {
	color: #262626;
	font-size: 14px;
	line-height:24px;
}
.ui-state-active {
	color:red;
}

/* Developer modifications */

.main-nav > li { height: 36px; }
.main-nav > li a { display: table-cell; height: 26px; vertical-align: middle; position: static; }

.banner-content.JEWELRY { margin: 26px -3px 10px 10px; }

.ws-filters .clear-all {
    background: url("../images/filter-remove.gif") repeat scroll 0 0 transparent;
    border: 0 none;
    height: 10px;
    width: 10px;
    cursor: pointer;
    display: block;
    float: right;
    font-size: 1px;
    line-height: 1px;
    position: relative;
    text-indent: -1000em;
    top: 2px;
}

.ws-filters .range {
    background-color: #FFFFFF;
    position: relative;
    width: 205px;
    margin: 0 0 31px 0;
}
.ws-filters .range a {
    background-color: #79756B;
    display: block;
    height: 8px;
    position: absolute;
    top: -3px;
    width: 2px;
}
.ws-filters .range div {
    background-color: #BBB9B2;
    font-size: 1px;
    height: 2px;
    line-height: 1px;
    position: absolute;
}
.ws-filters p.range-display {
    margin: 10px 0;
}
.ws-filters p.range-display em {
    font-style: normal;
}
.ws-filters p.range-display em.max {
    float: right;
}

.left-banner ul li { position: relative; }
.left-banner ul li .info { position: absolute; bottom: 20px; left: 20px;}
.left-banner ul li > a { display: block; width: 100%; height:100%; position: absolute; top: 0; left: 0;
  background: url("data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBR‌​AA7") 0 0 repeat; /* fix IE non-clickable empty anchor tags */
}

.custom-checkbox-alt .custom-checkbox {
  float: right;
  margin-right: 31px;
  margin-top: 33px !important;
  width: 201px;
}
.custom-checkbox-alt .custom-checkbox label {
  background-position: 162px -15px;
  color: #959595;
  font-family: georgia;
  font-size: 14px;
  padding-left: 0;
  padding-right: 27px;
  text-decoration: underline;
  text-transform: none;
  width: 174px !important;
}
.custom-checkbox-alt .custom-checkbox label.checked {
  background-position: 162px -215px;
  outline: none;
}
.custom-checkbox-alt .custom-checkbox input[type="checkbox"] {
  left: auto;
  right: 3px;
  top: 6px;
}

.detail-section .block { position: relative; overflow: visible; }
.detail-section .block label.error { font-size: 8pt; position: absolute; width: 300px; left: 208px; top: 23px; color: red; }

.detail-section .block.multi { display: inline-block; float: left; clear: none; }
.detail-section .block.multi label.error { left: 0; }
.detail-section .block.multi.first label.error { left: 208px; }

.detail-section .block .custom-select { overflow: visible; }
.detail-section .block .custom-select label.error { left: 0; margin-top: 7px; }

.login ul li { position: relative; overflow: visible; }
.login ul li label.error { font-size: 8pt; position: absolute; width: 240px; left: 206px; top: 23px; text-transform: uppercase; color: red; }

.message.error { color: #B3351E !important; margin-bottom: 10px; }
.message.error h3 { padding: 0 0 10px 0; color: #B3351E !important; }

.popup .custom-checkbox {
  float: none;
  margin-right: 0;
}
.popup .custom-checkbox label {
  padding-left: 33px;
}

a.delete { background: url("../images/cross.gif") no-repeat scroll 0 0 transparent; display: inline-block; height: 12px; width: 12px; }

.detail-section .cal-head input { text-align: center; }

.sidebar .products li { height: auto !important; }
.sidebar .products li h2 { position: static !important; }

/*==================================================
 WYSIWYG Styling
 ==================================================*/
.wysiwyg { color: #262626; font-family: Georgia; font-size: 15px; line-height: 19px; padding: 30px 0; }
.wysiwyg h1 { color: #607c74; font-family: "Gill Sans"; font-size: 24px; margin: 0 0 20px; text-transform: uppercase; }
.wysiwyg h2 { color: #b5985b; font-family: "Gill Sans"; font-size: 17px; line-height: 20px; text-transform: uppercase; }
.wysiwyg p { margin: 0 0 15px; }
.wysiwyg strong { color: inherit; font-size: inherit; line-height: inherit; }

/*==================================================
 Search Results
 ==================================================*/
.search-results { color: #262626; font-family: Georgia; font-size: 15px; line-height: 19px; padding: 30px 0; }
.search-results h1 { color: #607c74; font-family: "Gill Sans"; font-size: 24px; margin: 0 0 30px; text-transform: uppercase; }
.search-results .results li { margin: 0 0 30px; overflow: hidden; }
.search-results .results .pic { border: 1px solid #6d6e71; float: left; margin: 0 20px 0 0; padding: 10px; width: 90px; }
.search-results .results h2 { color: #b5985b; font-family: "Gill Sans"; font-size: 17px; line-height: 20px; margin: 15px 0 0; }
.search-results .pre-next { float: none; margin: 0; text-align: center; }