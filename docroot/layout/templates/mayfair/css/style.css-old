/*==================================================
 Custom Forms
 =================================================*/	 
.custom-checkbox, .custom-radio {
	position: relative;
	float:left;
	margin-right:10px;
	margin-top: 2px;
	_margin-top:20px;
}
.check-box .custom-checkbox input, .custom-checkbox input, .custom-radio input {
	position: absolute;
	left: 5px;
	top: 5px;
	margin: 0;
	z-index: 0;
    float: none;
    display: none;
}
.custom-checkbox label, .custom-radio label {
	width:auto;
	display: block;
	position: relative;
	height:8px;
	z-index: 1;
	font-size: 1.4em;
	padding-right: 1em;
	line-height: .2;
	padding:11px 0 8px 50px;
	margin: 0 0 .3em;
	cursor: pointer;
}
.custom-checkbox label {
	font-family: Georgia, "Times New Roman", Times, serif;
	font-size: 1.273em;
}
.popup .custom-radio {
  float: none;
  margin-right: 0;
}
.popup .custom-radio label {
  padding-left: 33px;
}
.small-caps .golden {
	font-size:14px;
}
.top-space .golden {
	font-size:14px;
	text-decoration:underline;
}
.top-space .golden:hover {
	text-decoration:none;
}
.custom-checkbox label {
	background: url(../images/checkbox.png) no-repeat;
	background-position:-8px -15px;
}
.custom-radio label {
	background: url(../images/radiobutton.png) no-repeat;
	background-position:-12px -16px;
	padding-left: 27px;
}
.custom-checkbox label.checked {
	background-position: -8px -215px;
}
.custom-radio label.checked {
	background-position: -12px -216px;
}
.custom-checkbox label.focus, .custom-radio label.focus {
	outline: 1px dotted #ccc;
}
